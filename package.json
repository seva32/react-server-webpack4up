{
  "name": "react-server-webpack",
  "version": "0.0.0",
  "description": "React apps",
  "main": "src/index.js",
  "scripts": {
    "start": "node --trace-deprecation server/server.js",
    "start:dashboard": "webpack-dashboard node server/server.js",
    "postinstall": "yarn run build && yarn run build:server",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "build:server": "webpack --config webpack/webpack.server.config.js",
    "clean": "rm -rf build",
    "clean:modules": "rm -rf node_modules",
    "call": "yarn run clean && yarn run clean:modules",
    "lint": "eslint src --ext .js*",
    "dev": "yarn run clean && yarn run build:server && yarn start",
    "test": "export NODE_ENV=test && jest --watch"
  },
  "author": "#",
  "private": true,
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/utils/jest/CSSStub.js",
      "^[./a-zA-Z0-9$_-]+\\.jpg$": "<rootDir>/src/utils/jest/ImgStub.js"
    },
    "displayName": {
      "name": "PROJECT",
      "color": "blue"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-jest": "^26.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "lodash": "^4.17.21",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.6",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.1.1",
    "universal-cookie-express": "^4.0.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx-source": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "compression": "^1.7.4",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss": "^6.0.12",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.1.2",
    "sass-loader": "^8.0.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  }
}
